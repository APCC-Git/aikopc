generator client {
  provider = "prisma-client-js"
  output   = "../src/types/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  user
  admin
}

model User {
  id       String @id @default(uuid())
  userId   String @unique
  username String @unique
  password String
  role     Role   @default(user)
}
